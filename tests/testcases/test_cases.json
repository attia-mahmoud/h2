      {
        "type": "HEADERS",
        "stream_id": 1,
        "flags": ["END_HEADERS", "END_STREAM"],
        "raw_payload": true,
        "headers": {
          "pseudo_headers": {
            "method": "GET",
            "scheme": "http",
            "path": "/",
            "authority": "example.com"
          },
          "regular_headers": {
            "x-null": "before\u0000after",
            "x-lf": "before\nafter",
            "x-cr": "before\rafter",
            "x-crlf": "before\r\nafter"
